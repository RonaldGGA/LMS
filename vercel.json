{
  "functions": {
    "app/api/**/*.ts": {
      "memory": 3008,
      "maxDuration": 30
    },
    "middleware.ts": {
      "runtime": "edge",
      "regions": ["iad1"]
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "X-Edge-Runtime", "value": "enabled" }]
    }
  ]
}
